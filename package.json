{
  "name": "newsfeed",
  "version": "0.0.0",
  "description": "A simple news feed app that displays news headlines from top news sites.",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "connect-mongo": "^1.3.2",
    "create-react-class": "^15.5.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "flux": "^2.1.1",
    "history": "^1.17.0",
    "mongoose": "^4.9.5",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-google-login": "^2.8.5",
    "react-router": "^1.0.3",
    "react-select": "^1.0.0-rc.3",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^3.0.1",
    "node-sass": "^4.5.2",
    "nyc": "^10.3.2",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.0"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom" : ["**/src/**.{js,jsx}", "!**/node_modules/**",
      "!**/vendor/**", "!**/src/configs/**", "!**/src/main.js", "!**/src/App.js"],
    "transform": {
      ".scss": "<rootDir>/node_modules/jest-css-modules",
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "watch": "./node_modules/.bin/webpack -d --watch",
    "build": "./node_modules/.bin/webpack",
    "dev": "webpack-dev-server --content-base src --inline --hot",
    "test": "nyc jest",
    "start": "node server.js",
    "coverage": "jest --coverage"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "**/*.test.jsx",
      "public"
    ],
    "extension": [
      ".jsx",
      ".es6"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-onnenanya/newsfeed.git"
  },
  "keywords": [
    "news",
    "newsfeed"
  ],
  "author": "Nnenanya O.K",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-onnenanya/newsfeed/issues"
  },
  "homepage": "https://github.com/andela-onnenanya/newsfeed#readme"
}
